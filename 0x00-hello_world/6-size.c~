#include <stdio.h>
/**
 *main - Entry point
 *prints some strig to the output
 *printf() writes a string to stdout
 *Return: Always 0 (Success)
 */

int main(void)
{
	char mychar;
	int myint;
	long int my_long_int;
	long long int my_long_long_i;
	float myfloat;
	printf("Size of a char: %i byte(s)\n", sizeof(mychar));
	printf("Size of an int: %i byte(s)\n",sizeof(myint));
	printf("Size of of a long int: %i byte(s)\n",sizeof(my_long_int));
	printf("Size of a long long int: %i byte(s)\n", sizeof(my_long_long_i));
	printf("Size of a float: %i bytes(s)\n", sizeof(myfloat));
	return (0);
}
